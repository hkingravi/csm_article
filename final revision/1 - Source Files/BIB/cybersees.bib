	Issn = {1476-{945X}},
	Journal = {Ecological Complexity},
	Keywords = {Ecosystem control, Ecosystem design, Ecosystem services, Extremal dynamics, Indirect effects, Multivariable control, Network mutualism, Network synergism, Steady-state dynamics},
	Month = sep,
	Number = {3},
	Pages = {282--291},
