	Journal = {Nature},
	Month = dec,
	Number = {6659},
	Pages = {507--509},
	Title = {Biodiversity enhances ecosystem reliability},
	Url = {http://www.nature.com.proxy2.library.illinois.edu/nature/journal/v390/n6659/abs/390507a0.html},
