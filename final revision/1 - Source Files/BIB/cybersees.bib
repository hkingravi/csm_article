	Issn = {0962-8452, 1471-2954},
	Journal = {Proceedings of the Royal Society B: Biological Sciences},
	Keywords = {biodiversity--ecosystem function, functional complementarity, habitat loss, seed rain, species richness, Turdus spp.; diversity},
	Language = {en},
