	Pages = {237--258},
	Shorttitle = {Measurements and Modeling of Carbon and Nitrogen Cycling in Agroecosystems of Southern Wisconsin},
	Title = {Measurements and modeling of carbon and nitrogen cycling in agroecosystems of southern wisconsin: Potential for {SOC} sequestration during the next 50 years},
