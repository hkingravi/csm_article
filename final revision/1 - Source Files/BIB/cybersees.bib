	Keywords = {coffee berry borer, coffee farms, ecosystem services, Hypothenemus hampei, Jamaican coffee, pest control; diversity},
	Language = {en},
	Number = {5},
	Pages = {1177--1185},
	Title = {Ecological and economic services provided by birds on jamaican blue mountain coffee farms},
	Url = {http://onlinelibrary.wiley.com.proxy2.library.illinois.edu/doi/10.1111/j.1523-1739.2008.00968.x/abstract},
