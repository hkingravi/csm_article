	Issn = {0018-9219},
	Journal = {Proceedings of the IEEE},
	Month = {December},
	Number = {99},
	Pages = {1--12},
	Title = {{The Impact of Human-Automation Collaboration in Decentralized Multiple Unmanned Vehicle Control}},
	Url = {http://dx.doi.org/10.1109/JPROC.2011.2174104},
	Year = {2011},
