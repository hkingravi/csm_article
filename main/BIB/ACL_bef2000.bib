	Keywords = {Lyapunov methods; control system analysis; matrix algebra; multidimensional systems; optimal control; stability; time-varying systems; LMI; LPV system analysis; PAPDLF; computationally intensive approach; continuous piecewise affine parameter-dependent Lyapunov function; finite-dimensional linear matrix inequality; induced L$_{2}$ performance analysis; linear parameter-varying systems; nonsmooth dissipative system theory; parameter-dependent Lyapunov functions; stability},
	Pages = {978--983},
