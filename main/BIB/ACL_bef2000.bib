@article{osterreicher93,
	Abstract = { In analogy with the definition of Shannon information, M.H. De Groot
	(1962) defined statistical information as the difference between
	prior and posterior risk of a statistical decision problem. Relations
	are studied between the statistical information and the discrimination
	functions of information theory known as f-divergences. Using previous
	results, it is shown that every f-divergence I/sub f/(P,Q) is an
	average statistical information or decision problem with dichotomic
	parameter, 0-1 loss function, and corresponding observation distributions
	P and Q. The average is taken over a distribution on the parameter's
	prior probability. This distribution is uniquely determined by the
	function f. The main result is that every f-divergence is statistical
	information in some properly chosen statistical decision problem,
	and conversely, that every piece of statistical information is an
	f-divergence. This provides a new representation of discrimination
	functions figuring in signal detection, data compression, coding
	pattern classification, cluster analysis, etc (22 Refs.)},
	Author = {Osterreicher, F. and Vajda, I.},
	Date-Added = {2011-03-31 09:08:55 -0400},
	Date-Modified = {2011-03-31 09:08:56 -0400},
	Descriptors = {decision theory; information theory; statistical analysis},
	Journal = {IEEE Transactions on Information Theory},
	Keywords = {Shannon information; statistical information; statistical decision problem; discrimination functions; information theory; f-divergence; dichotomic parameter; observation distributions; signal detection; data compression; coding pattern classification; cluster analysis},
	Month = {May},
	Number = {3},
	Pages = {1036-9},
	Title = {Statistical information and discrimination},
	Volume = {39},
	Year = {1993}}

