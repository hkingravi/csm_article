	Keywords = {Lyapunov methods, control nonlinearities, control system analysis, matrix algebra, stability criteria, L<sub>2</sub>-gain, LMI, Lyapunov functions, Popov criteria, circle criteria, convex optimization, discrete time systems, disturbance rejection, guaranteed performance regions, guaranteed stability regions, linear matrix inequalities, linear system analysis, local stability analysis, performance analysis, saturation},
	Owner = {Jonathan P. How},
