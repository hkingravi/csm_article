@conference{SimRoy_ICRA_2005,
	Abstract = {It is well-known that the Kalman filter for simultaneous localization
	and mapping (SLAM) converges to a fully correlated map in the limit
	of infinite time and data [REF_REMOVED]. However, the rate of convergence
	of the map has a strong dependence on the order of the observations.
	We show that conventional exploration algorithms for collecting map
	data are sub-optimal in both the objective function and choice of
	optimization procedure. We show that optimizing the a-optimal information
	measure results in a more accurate map than existing approaches,
	using a greedy, closed-loop strategy. Secondly, we demonstrate that
	by restricting the planning to an appropriate policy class, we can
	tractably find non-greedy, global planning trajectories that produce
	more accurate maps, explicitly planning to close loops even in open-loop
	scenarios.},
	Author = {Robert Sim and Nicholas Roy},
	Booktitle = icra,
	Date-Added = {2010-08-18 13:27:05 -0400},
	Date-Modified = {2010-08-18 13:27:05 -0400},
	Owner = {dlevine},
	Review = {Brief but satisfactory motivation for using the A-optimality metric; refer to this paper more for better description of the difference between A- and D-optimality metrics. Also provides an interesting global planner for grid world without sensor obstacle occlusion ( and possibly without FOV) issues. Describes the problem in terms of a {{POMDP}}, then skips {{POMDP}} approximate solution methods in favor of graph-search methods that exploit structure of the environment, as well as an imposed prohibition on state revisitation, which eases the exponential computational burden. Questions arise over degree of performance loss as a result of the state revisitation prohibition; and the need for a replanning strategy, which is not employed for computational reasons.},
	Timestamp = {2010.01.19},
	Title = {Global A-Optimality Robot Exploration in {SLAM}},
	Year = {2005}}

