	Journal = {Acoustics, Speech and Signal Processing, 2008. ICASSP 2008. IEEE International Conference on},
	Keywords = {error statistics, trees (mathematics)alternative forests, complex graphical models, efficient max-weight spanning tree algorithm, error probability, max-weight discriminative forests, multivalued edge-weights, sequential learning},
	Month = {31 2008-April 4},
