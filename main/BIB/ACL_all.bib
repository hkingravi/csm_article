@inproceedings{Fox06_ACSSC,
	Abstract = {We consider the problem of data association for multi-target tracking
	in the presence of an unknown number of targets. For this application,
	inference in models which place parametric priors on large numbers
	of targets becomes computationally intractable. As an alternative
	to parametric models, we explore the utility of nonparametric Bayesian
	methods, specifically Dirichlet processes, which allow us to put
	a flexible, data-driven prior on the number of targets present in
	our observations. Dirichlet processes provide a prior on partitions
	of the observations among targets whose dynamics are individually
	described by state space models. These partitions represent the tracks
	with which the observations are associated. We provide preliminary
	data association results for the implementation of Dirichlet processes
	in this scenario.},
	Author = {Fox, E.B. and Choi, D.S. and Willsky, A.S.},
	Booktitle = {Fortieth Asilomar Conference on Signals, Systems and Computers (ACSSC '06)},
	Date-Added = {2012-01-13 13:15:57 -0500},
	Date-Modified = {2012-01-13 13:22:08 -0500},
	Doi = {10.1109/ACSSC.2006.355118},
	Issn = {1058-6393},
	Keywords = {Dirichlet processes;data association;large scale multi-target tracking;nonparametric Bayesian methods;state space models;Bayes methods;nonparametric statistics;target tracking;},
	Month = {Nov},
	Pages = {2009 -2013},
	Title = {Nonparametric Bayesian Methods for Large Scale Multi-Target Tracking},
	Year = {2006},
